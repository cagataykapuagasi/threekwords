{
  "name": "3KWords",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "release": "react-native run-android --variant=release",
    "bundle": "cd android && ./gradlew bundleRelease",
    "debugApk": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && ./gradlew assembleDebug",
    "ios": "react-native run-ios --simulator='iPhone 8'",
    "ios2": "react-native run-ios --simulator='iPhone 8 Plus'",
    "pod": "cd ios && pod install",
    "reset": "react-native start --reset-cache",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
